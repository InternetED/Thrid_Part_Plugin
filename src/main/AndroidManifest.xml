<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pikolive.module">

    <uses-permission android:name="android.permission.INTERNET" />

    <application>


        <provider
            android:name=".ad.AdManagerProvider"
            android:authorities="${applicationId}.admanagerprovider"
            android:exported="false" />

        <provider
            android:name=".lifecycle.AppManagerProvider"
            android:authorities="${applicationId}.appmanagerprovider"
            android:exported="false" />

        <provider
            android:name=".login.LoginManagerProvider"
            android:authorities="${applicationId}.loginmanagerprovider"
            android:exported="false" />


        <provider android:authorities="com.facebook.app.FacebookContentProvider{298775961167198}"
            android:name="com.facebook.FacebookContentProvider"
            android:exported="true"/>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_id" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_id" />



    </application>
</manifest>
